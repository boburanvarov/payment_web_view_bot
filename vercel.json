{
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "https://api.kartaxabar.uz/api/:path*"
        }
    ],
    "headers": [
        {
            "source": "/api/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "no-cache, no-store, must-revalidate"
                }
            ]
        }
    ]
}